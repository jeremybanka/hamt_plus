{
  "dependencies": {
    "hamt_plus": ".."
  },
  "devDependencies": {
    "@banka/eslint-config": "1.1.6",
    "@types/chai": "^4.3.4",
    "@types/mocha": "10.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "chai": "4.3.7",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^25.0.0",
    "eslint-plugin-jsx-a11y": "^6.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.22.0",
    "prettier": "^2.7.1",
    "typescript": "4.9.3"
  },
  "eslintConfig": {
    "extends": [
      "@banka"
    ],
    "rules": {
      "import/extensions": [
        "error",
        {
          "ts": "never",
          "tsx": "never",
          "js": "never",
          "jsx": "never",
          "css": "always",
          "scss": "always",
          "client": "always",
          "server": "always",
          "schema": "always",
          "json": "always"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/*.d.ts"
        ],
        "rules": {
          "semi": [
            "error",
            "always"
          ],
          "quotes": [
            "error",
            "double"
          ]
        }
      }
    ]
  }
}
